Algoritmo "Verificar se um ano é bissexto"

Var

   ano: inteiro

Inicio

   //Input
   escreva("Digite um ano: ")
   leia(ano)
   escreval
   se(((ano%4=0) e (ano%100<>0)) ou (ano%400=0)) entao
      escreva("O ano", ano, " é bissexto!")
   senao
      escreva("O ano", ano, " não é bissexto!")
   fimse

Fimalgoritmo